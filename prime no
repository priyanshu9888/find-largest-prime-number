def primeno(x):
    import sympy
    sum=0
    list1=[]
    for num in range(1,x):
        if sympy.isprime(num) is True:
            sum=num+num
            list1.append(sum)
    list2=[]
    for sum in list1:
        if sum<x:
            if sympy.isprime(sum)==True:
                list2.append(sum)
    return max(list2)
x= int(input("enter a number:"))
primeno(x)
